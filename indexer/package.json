{
  "name": "prophet-indexer",
  "version": "0.1.0",
  "scripts": {
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "mocha": "ts-mocha test/**/*.ts",
    "codegen": "CI=true envio codegen",
    "dev": "envio dev",
    "prestart": "if [ -n \"$SKIP_CODEGEN\" ]; then echo \"Skipping codegen (SKIP_CODEGEN is set)\"; else npm run codegen; fi",
    "start": "envio start",
    "test": "npm run mocha"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "ts-mocha": "^10.0.0",
    "typescript": "5.2.2",
    "chai": "4.3.10",
    "mocha": "10.2.0"
  },
  "dependencies": {
    "envio": "2.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
